{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "soudPass",
    "soundVolume",
    "soundRadius"
  ],
  "properties": {
    "soudPass": {
      "type": "string",
      "default": "",
      "title": "The soudPass Schema",
      "examples": [
        "data/sound/space-fighter-taking-off-1.mp3"
      ]
    },
    "soundVolume": {
      "type": "number",
      "default": 0.0,
      "title": "The soundVolume Schema",
      "examples": [
        200.0
      ]
    },
    "soundRadius": {
      "type": "number",
      "default": 0.0,
      "title": "The soundRadius Schema",
      "examples": [
        300.0
      ]
    }
  },
  "examples": [
    {
      "soudPass": "data/sound/space-fighter-taking-off-1.mp3",
      "soundVolume": 200.0,
      "soundRadius": 300.0
    }
  ]
}